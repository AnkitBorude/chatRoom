{
  "compilerOptions": {
    
    "target": "es2016" ,                                 
    "moduleResolution": "node",                     
    "baseUrl": "./",                                 
    "paths": {
      "@shared/*": ["shared/*"]
    },                                     
    "resolveJsonModule": true,                      
    "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
    "strict": true,       
    "skipLibCheck": true
  }
}
